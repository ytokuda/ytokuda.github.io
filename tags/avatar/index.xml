          &lt;source src=&#34;jyugyou.webm&#34; type=&#34;video/webm&#34; class=&#34;mx-auto d-block&#34;/&gt;
           &lt;source src=&#34;jyugyou.mp4&#34; type=&#34;video/mp4&#34; class=&#34;mx-auto d-block&#34;/&gt;
           &lt;img src=&#34;https://res.cloudinary.com/dqmr9nqcy/image/upload/f_auto,fl_progressive,q_auto:best/v1584772442/homepage/mobileavatar/jyugyou_mzf6nr.gif&#34; class=&#34;mx-auto d-block&#34;&gt;
         &lt;/video&gt;
         &lt;p class=&#34;lead&#34;&gt; Remote Lecture from Home &lt;/p&gt;
        &lt;/div&gt;
        &lt;div class=&#34;col-md-4&#34;&gt;
          &lt;video muted autoplay loop poster=&#34;https://res.cloudinary.com/dqmr9nqcy/image/upload/f_auto,fl_progressive,q_auto:best/v1584772442/homepage/mobileavatar/bearanimation_bwztxf.jpg&#34; width=&#34;370&#34; height=&#34;231&#34;&gt;
          &lt;source src=&#34;bearanimation.webm&#34; type=&#34;video/webm&#34; class=&#34;mx-auto d-block&#34;/&gt;
           &lt;source src=&#34;bearanimation.mp4&#34; type=&#34;video/mp4&#34; class=&#34;mx-auto d-block&#34;/&gt;
